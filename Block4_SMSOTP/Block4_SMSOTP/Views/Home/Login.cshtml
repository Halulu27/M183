
@{
    ViewBag.Title = "Log In";
}

<div class="row" style="margin-top: 80px;">
    <div class="col-lg-8 col-lg-offset-2">
        <label>Token Log In</label>
        @ViewBag.Message

        <form class="well" action="./Home/TokenLogin" method="POST">
            <label><small>Token</small></label>
            <input type="text" name="token" value="" class="form-control" />
            <br />
            <input type="submit" name="" value="Verfiy" class="btn btn-primary" />
        </form>
    </div>
</div>
